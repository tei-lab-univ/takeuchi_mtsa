/*Fluent*/
fluent REPLENISH[i:PodID] = <replenish[i], [ReplenishRobot].move[i]>
fluent BRING[i:PodID]     = <bring[i], [BringItemRobot].move[i]>

fluent PROCESS_REQUEST    = <RequestActions, DoneActions>
fluent PROCESS_REPLENISH_ITEM = <reqReplenishItem, doneReplenishItem>
fluent PROCESS_BRING_ITEM = <reqBringItem, doneBringItem>
fluent PROCESS_BRING_BOX  = <reqBringBox, doneBringBox>
fluent PROCESS_SHIP       = <reqShip, doneShip>

fluent REQ_SHIP_TIMEING = <pack, doneShip>

fluent SHIP_TIMING   = <[ShippingRobot].put['shippingArea], doneShip>
fluent BOX_TIMING    = <[BringBoxRobot].move['boxGettingArea], [BringBoxRobot].pick['boxGettingArea]>
fluent ITEM_TIMING   = <[ReplenishRobot].put['itemGettingArea], [ReplenishRobot].pick['itemGettingArea]>
fluent GET_TIMING_1  = <[BringItemRobot].put['packingArea], [BringItemRobot].pick['packingArea]>
fluent GET_TIMING_2  = <doneBringBox, [BringItemRobot].put['packingArea]>
fluent PACK_TIMING_1 = <doneBringBox, reqShip>
fluent PACK_TIMING_2 = <doneBringItem, reqShip>

fluent RETURN_BRINGITEM[i:PodID]   = <[BringItemRobot].pick[i], [BringItemRobot].move[i]>
fluent RETURN_REPLENISH[i:PodID]   = <[ReplenishRobot].pick[i], [ReplenishRobot].move[i]>

